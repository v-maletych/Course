print(2+2)

#EXERCISE_2

#take a max number
num_max=[1,2,3,4,5,6,7,8,9]
largest_number=num_max[0]
for number in num_max:
    if number>largest_number:
        largest_number=number
print(largest_number)

#take a min number
num_min=[1,2,3,4,5,6,7,8,9]
lowest_number=num_min[0]
for number_min in num_min:
    if number_min<lowest_number:
        lowest_number==number_min
print(lowest_number)

#EXERCISE_3

world="hello"
reversed="".join(reversed(world))
print(reversed)
